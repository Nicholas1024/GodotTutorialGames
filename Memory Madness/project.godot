;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Memory Madness"
run/main_scene="res://MEM/scenes/TtlMEMMasterScene/TtlMEMMasterScene.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://icon.png"

[autoload]

TtlMEMSignalManager="*res://MEM/singletons/TtlMEMSignalManager.gd"
TtlMEMImageManager="*res://MEM/singletons/TtlMEMImageManager.gd"
TtlMEMSoundManager="*res://MEM/singletons/TtlMEMSoundManager.gd"
TtlMEMGameManager="*res://MEM/singletons/TtlMEMGameManager.gd"

[display]

window/size/viewport_width=1024
window/size/viewport_height=768
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="Memory Madness"

[rendering]

renderer/rendering_method="mobile"
